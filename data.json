{
  "providers": {
    "openai": {
      "apiKey": "secret:markdown-next-ai-api-key-openai",
      "baseUrl": "https://yunwu.ai/v1",
      "enabled": true,
      "apiKeyLink": "https://platform.openai.com/api-keys"
    }
  },
  "models": {
    "gemini-3-pro-preview": {
      "id": "gemini-3-pro-preview",
      "name": "gemini-3-pro-preview",
      "provider": "openai",
      "model": "gemini-3-pro-preview",
      "enabled": true,
      "category": "multimodal",
      "actualModel": "gemini-3-pro-preview"
    },
    "gemini-3-flash-preview": {
      "id": "gemini-3-flash-preview",
      "name": "gemini-3-flash-preview",
      "provider": "openai",
      "model": "gemini-3-flash-preview",
      "enabled": true,
      "category": "multimodal",
      "actualModel": "gemini-3-flash-preview"
    },
    "gpt-5": {
      "id": "gpt-5",
      "name": "GPT-5",
      "provider": "openai",
      "model": "gpt-5",
      "enabled": true,
      "category": "multimodal",
      "actualModel": "gpt-5",
      "type": "chat"
    },
    "deepseek-chat": {
      "id": "deepseek-chat",
      "name": "deepseek-chat",
      "provider": "openai",
      "model": "deepseek-chat",
      "actualModel": "deepseek-chat",
      "enabled": true,
      "category": "multimodal"
    }
  },
  "currentModel": "gemini-3-flash-preview",
  "timeout": 30000,
  "enableRightClick": true,
  "enableAtTrigger": true,
  "maxTokens": 5000,
  "maxContextLines": 20,
  "maxContextChars": 3000,
  "globalRules": [],
  "ruleTemplates": [],
  "enableGlobalRules": true,
  "commonPrompts": [
    {
      "id": "expand",
      "name": "内容扩充",
      "content": "基于现有内容进行深度扩充，补充更多细节描述及具体案例，使其更加丰富完整。"
    },
    {
      "id": "summarize",
      "name": "核心摘要",
      "content": "提炼这段文本的核心观点，生成一份精简的摘要，保留关键信息。"
    },
    {
      "id": "improve",
      "name": "润色优化",
      "content": "优化这段文字的表达方式，使其逻辑更通顺，用词更精准，提升整体可读性。"
    },
    {
      "id": "translate",
      "name": "中译英",
      "content": "翻译以上内容"
    },
    {
      "id": "continue",
      "name": "续写创作",
      "content": "承接上文语境和风格，继续撰写后续内容，保持思路连贯。"
    }
  ],
  "lastInsertAction": "insert",
  "baseModelSpecialPrompt": "",
  "tabCompletion": {
    "enabled": true,
    "modelId": "deepseek-chat",
    "systemPrompt": "You are a text completion engine. Your task is to complete the text at the cursor position marked by <mask/>. Output ONLY the completion text, no explanation, no markdown code blocks unless the completion itself is code.",
    "maxSuggestionLength": 100,
    "contextRange": 2000,
    "idleTriggerEnabled": true,
    "autoTriggerDelayMs": 500,
    "triggerDelayMs": 2000,
    "autoTriggerCooldownMs": 0,
    "triggers": [
      {
        "type": "regex",
        "pattern": "\\.$",
        "enabled": true
      },
      {
        "type": "regex",
        "pattern": "\\n$",
        "enabled": true
      },
      {
        "type": "regex",
        "pattern": "\\($",
        "enabled": true
      },
      {
        "type": "regex",
        "pattern": "\\[$",
        "enabled": true
      },
      {
        "type": "regex",
        "pattern": ":$",
        "enabled": true
      },
      {
        "type": "regex",
        "pattern": "=$",
        "enabled": true
      }
    ],
    "lengthPreset": "medium"
  },
  "useKeychain": true,
  "conversationHistory": [],
  "conversationHistoryLimit": 50
}